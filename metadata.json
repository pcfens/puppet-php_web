{
  "name": "pcfens-php_web",
  "version": "0.1.1",
  "author": "pcfens",
  "summary": "Create and manage vhosts and and php-fpm pools from a single definition.",
  "license": "Apache-2.0",
  "source": "https://github.com/pcfens/puppet-php_web",
  "project_page": "https://github.com/pcfens/puppet-php_web",
  "issues_url": "https://github.com/pcfens/puppet-php_web/issues",
  "description": "Manage php-fpm and apache virtualhosts",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5.0",
        "6.0"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=3.0.0 < 4.1.0"
    },
    {
      "name": "pe",
      "version_requirement": "3.x"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.0.0 <5.0.0"},
    {"name":"puppetlabs/apache","version_requirement":">= 1.3.0 <2.0.0"},
    {"name":"thias/php","version_requirement":">= 1.0.0 <2.0.0"}
  ]
}
